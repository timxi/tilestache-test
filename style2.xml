<?xml version="1.0"?>

<!-- An ultra simple Mapnik stylesheet -->

<Map>
    <Style name="style">
        <Rule>
            <PolygonSymbolizer />
        </Rule>
    </Style>
    <Layer name="layer">
        <StyleName>style</StyleName>
        <Datasource>
            <Parameter name="type">shape</Parameter>
            <Parameter name="file">sample_data/tl_2020_us_state.shp</Parameter>
        </Datasource>
    </Layer>
</Map>